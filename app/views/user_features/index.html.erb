<p id="notice"><%= notice %></p>

<h1>User Features</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_features.each do |user_feature| %>
      <tr>
        <td><%= link_to 'Show', user_feature %></td>
        <td><%= link_to 'Edit', edit_user_feature_path(user_feature) %></td>
        <td><%= link_to 'Destroy', user_feature, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User Feature', new_user_feature_path %>
